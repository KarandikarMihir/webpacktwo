{
  "name": "webpacktwo",
  "version": "1.0.0",
  "description": "Webpack 2 playground",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --open --config ./webpack/dev.babel.js",
    "build:webpack": "webpack --config ./webpack/dev.babel.js",
    "build:dll": "webpack --config ./webpack/dll.babel.js",
    "build": "npm run clean && npm run build:dll && npm run build:webpack",
    "clean": "rimraf dist ./node_modules/example-dll",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarandikarMihir/webpacktwo.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Mihir Karandikar <karandikar.mihir@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KarandikarMihir/webpacktwo/issues"
  },
  "homepage": "https://github.com/KarandikarMihir/webpacktwo#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.24.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
